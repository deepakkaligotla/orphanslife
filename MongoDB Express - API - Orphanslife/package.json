{
  "name": "orphanslife",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "api": "nodemon ./API_Auth/API_Auth.js",
    "admin": "nodemon ./Services/Service_Admin/Service_Admin_Index.js",
    "sponsor": "nodemon ./Services/Service_Sponsor/Service_Sponsor_Index.js",
    "orphanage": "nodemon ./Services/Service_Orphanage/Service_Orphanage_Index.js",
    "business": "nodemon ./Services/Service_Business/Service_Business_Index.js",
    "extras": "nodemon ./Services/Service_Extras/Service_Extras_Index.js",
    "start": "concurrently \"npm run api\" \"npm run admin\" \"npm run business\" \"npm run extras\" \"npm run orphanage\"",
    "stop": "concurrently \"sudo lsof -t -i tcp:80 -s tcp:listen | sudo xargs kill\" \"sudo lsof -t -i tcp:1000 -s tcp:listen | sudo xargs kill\" \"sudo lsof -t -i tcp:2000 -s tcp:listen | sudo xargs kill\" \"sudo lsof -t -i tcp:3000 -s tcp:listen | sudo xargs kill\" \"sudo lsof -t -i tcp:4000 -s tcp:listen | sudo xargs kill\" \"sudo lsof -t -i tcp:6000 -s tcp:listen | sudo xargs kill\"",
    "react": "npm start --prefix client",
    "dev": "concurrently \"npm run react\" \"npm run start\"",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "aws-sdk": "^2.1299.0",
    "config": "^3.3.9",
