<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.view.sponsor_home.SponsorHome">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar_top"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@color/textBlack"
        android:minHeight="?android:attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </androidx.appcompat.widget.Toolbar>



    <TextView
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:fontFamily="@font/nunito_bold"
        android:text="Sponsor Home"
        android:textColor="@color/darkBrown"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_top"
        app:layout_constraintEnd_toEndOf="@+id/toolbar_top"/>

    <TextView
        android:id="@+id/tokenPref"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:fontFamily="@font/nunito_bold"
        android:text=""
        android:textColor="@color/darkBrown"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/title"
        app:layout_constraintEnd_toEndOf="@+id/title"/>

    <TextView
        android:id="@+id/idPref"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:fontFamily="@font/nunito_bold"
        android:text=""
        android:textColor="@color/darkBrown"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/tokenPref"
        app:layout_constraintEnd_toEndOf="@+id/tokenPref"/>

</androidx.constraintlayout.widget.ConstraintLayout>